		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 21488]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 21489]
				subType=enum:0
				text=string:Tu es comme un grand frère pour moi. Je n'ai jamais eu de grand frère. Ou de petit. Mes parents ne voulaient pas de moi. Mais je suis sûr que ça a été dur pour eux.
				name=string:DIA_Mud_Nerve_14_07_00.WAV
			[]
			synchronized=bool:1
