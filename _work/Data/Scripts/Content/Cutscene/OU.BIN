		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 10766]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 10767]
				subType=enum:0
				text=string:Ne sens-tu pas tous les jours pousser en toi un impérieux besoin de liberté ? Ne le sens-tu pas grandir de plus en plus ? Il guide ton esprit. Laisse-le faire !	
				name=string:DIA_BaalTondral_FanaticTeacher_13_04.WAV
			[]
			synchronized=bool:1
		[]
	[]
	[% zCCSBlock 0 10768]
		blockName=string:DIA_BaalTondral_FanaticTeacher_13_05
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 10769]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 10770]
				subType=enum:0
				text=string:Sais-tu ce que tu fais quand tu tentes de le supprimer ? Tu renies ton âme, en te reniant toi-même ! Ne laisse pas tes peurs dominer ta vie.	
				name=string:DIA_BaalTondral_FanaticTeacher_13_05.WAV
			[]
			synchronized=bool:1
