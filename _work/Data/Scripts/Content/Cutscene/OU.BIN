		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 21473]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 21474]
				subType=enum:0
				text=string:Ces salopards du Nouveau Camp seront étonnés de savoir que j'ai un protecteur. Ils feront dans leur froc. Ce sera marrant. Normalement ils exécutent les gens rapidement, mais la ils y réfléchiront à deux fois.
				name=string:DIA_Mud_Nerve_9_07_00.WAV
			[]
			synchronized=bool:1
		[]
	[]
	[% zCCSBlock 0 21475]
