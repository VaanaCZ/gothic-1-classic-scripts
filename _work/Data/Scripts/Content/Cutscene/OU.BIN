		[% zCCSAtomicBlock 0 21470]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 21471]
				subType=enum:0
				text=string:Il y a des gens qui essayent constamment de me frapper. Ces abrutis du Nouveau Camp. Un vrai coupe-gorge. La prochaine fois je leurs dirai que tu es avec moi. Alors ils auront peurs.
				name=string:DIA_Mud_Nerve_8_07_00.WAV
			[]
			synchronized=bool:1
		[]
	[]
	[% zCCSBlock 0 21472]
