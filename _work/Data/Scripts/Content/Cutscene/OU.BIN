		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 21506]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 21507]
				subType=enum:0
				text=string:Nous faisons une fine équipe. Nous pourrions prendre d'assaut le Vieux Camp. Les Barons du minerai ne devraient pas nous poser de problème ensemble. Je vais élaborer un plan.
				name=string:DIA_Mud_Nerve_19_07_00.WAV
			[]
			synchronized=bool:1
