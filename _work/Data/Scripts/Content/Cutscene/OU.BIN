		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 21476]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 21477]
				subType=enum:0
				text=string:Tu sais quoi ? Je vais te rendre un service. Je vais parler de toi en bon terme, tu seras peut-être admis au Camp. Alors nous pourrons nous voir tous les jours.
				name=string:DIA_Mud_Nerve_10_07_00.WAV
			[]
			synchronized=bool:1
		[]
	[]
	[% zCCSBlock 0 21478]
		blockName=string:DIA_Mud_Nerve_11_07_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 21479]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 21480]
				subType=enum:0
				text=string:Certaines personnes pensent que je suis complètement lunatique. Mais quelqu'un comme toi ne serait jamais avec un lunatique. Les gens le verront. Et alors ils comprendront qu'ils se trompaient.
				name=string:DIA_Mud_Nerve_11_07_00.WAV
			[]
			synchronized=bool:1
		[]
	[]
	[% zCCSBlock 0 21481]
