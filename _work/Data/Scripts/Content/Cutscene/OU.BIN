		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 10763]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 10764]
				subType=enum:0
				text=string:Avec ces mensonges ils essayent de contrôler des esprits faibles. Mais ton esprit est plus fort que leurs mensonges !	
				name=string:DIA_BaalTondral_FanaticTeacher_13_03.WAV
			[]
			synchronized=bool:1
		[]
	[]
	[% zCCSBlock 0 10765]
